        <activity
            android:name="com.example.trial3.M5"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.trial3.M6"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.trial3.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.FLAG." />
                <action android:name="com.example.trial3.Main1Activity" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.trial3.ExtraNetAct"
            android:label="@string/title_activity_extra_net" >
